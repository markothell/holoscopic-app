{
  "name": "holoscopic-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:interactive": "node interactive-test.js",
    "test:load": "node enhanced-load-test.js",
    "test:load-light": "CONCURRENT_USERS=10 TEST_DURATION_MS=15000 node enhanced-load-test.js",
    "test:load-heavy": "CONCURRENT_USERS=50 TEST_DURATION_MS=60000 node enhanced-load-test.js",
    "test:simulate": "node multi-user-simulator.js",
    "test:simulate-small": "NUM_USERS=5 node multi-user-simulator.js",
    "test:simulate-large": "NUM_USERS=25 node multi-user-simulator.js",
    "test:all": "npm run test:interactive && npm run test:load-light && npm run test:simulate-small",
    "load-test": "node load-test.js",
    "load-test-production": "API_BASE_URL=https://we-all-explain-socket-server.onrender.com node load-test.js"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "mongodb": "^6.19.0",
    "mongoose": "^8.16.2",
    "next": "15.3.5",
    "next-auth": "^4.24.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
